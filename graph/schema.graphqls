type Query {
  getCreatorDetails: CreatorDetails!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
}

""" Time formated in the time.RFC3339Nano Format """
scalar Time

input NewTodo {
  text: String!
  userId: String!
}


type Todo {
  text: String!
  userId: String!
}


type CreatorDetails {
  creatorID: ID!
  address: String!
}
