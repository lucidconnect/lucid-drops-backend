type Query {
  getCreatorDetails: CreatorDetails!
}

type Mutation {
  createCollection(input: NewCollection!): Collection!
}

""" Time formated in the time.RFC3339Nano Format """
scalar Time

input NewCollection {
  name: String!
  contentURI: String!
}


type Collection {
  ID: ID!
  name: String!
  contentURI: String!
}


type CreatorDetails {
  creatorID: ID!
  address: String!
}
